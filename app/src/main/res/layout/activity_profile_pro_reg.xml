<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.patrick.aspro.ProfileProRegActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="0dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="0dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp">

        <include
            android:id="@+id/toolbar"
            layout="@layout/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:maxHeight="20dp"
            android:maxWidth="20dp"
            app:srcCompat="@drawable/user"
            tools:layout_editor_absoluteX="130dp"
            tools:layout_editor_absoluteY="56dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Nome"
            android:textAlignment="center"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="194dp" />

        <EditText
            android:id="@+id/et_profilePro_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="386dp"
            tools:layout_editor_absoluteY="219dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="email"
            android:textAlignment="center"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="264dp" />

        <EditText
            android:id="@+id/et_profilePro_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="386dp"
            tools:layout_editor_absoluteY="292dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Profissão:"
            android:textAlignment="center"
            android:textSize="14sp"

            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="341dp" />

        <Spinner
            android:id="@+id/spinner_profilePro_pro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="156dp"
            tools:layout_editor_absoluteY="361dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Valor"
            android:textAlignment="center"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="164dp"
            tools:layout_editor_absoluteY="393dp" />

        <EditText
            android:id="@+id/et_profilePro_wage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="R$/Hora"
            android:inputType="number"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="97dp"
            tools:layout_editor_absoluteY="424dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Descrição:"
            android:textAlignment="center"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="44dp"
            tools:layout_editor_absoluteY="468dp" />

        <EditText
            android:id="@+id/et_profilePro_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:elevation="1dp"
            android:ems="10"
            android:inputType="textMultiLine"
            android:selectAllOnFocus="false"
            android:singleLine="false"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="97dp"
            tools:layout_editor_absoluteY="489dp" />

        <Button
            android:id="@+id/bt_perfilPro_update"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Atualizar"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="535dp" />

    </LinearLayout>


</android.support.constraint.ConstraintLayout>
